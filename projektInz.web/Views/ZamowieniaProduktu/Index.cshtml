@model System.Collections.Generic.List<projektInz.web.Models.WidokZamowieniaProduktu>

@{
    ViewBag.Title = "Zamówienia";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h2>Produkty</h2>

        <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Produkt</th>
                        <th>jm.</th>
                        <th>Ilość</th>
                        <th>Data złożenia</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var zamowienie in this.Model)
                    {
                        <tr>
                            <td>@zamowienie.Produkt</td>
                            <td>@zamowienie.JM</td>
                            <td>@zamowienie.Ilosc</td>
                            <td>@zamowienie.DataZlozenia</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>

