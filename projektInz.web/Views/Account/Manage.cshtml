@model projektInz.web.Models.WidokUżytkownika
@{
    ViewBag.Title = "Manage Account";
}

<hgroup class="title">
    <h1>@ViewBag.Title.</h1>
</hgroup>

<p class="message-success">@ViewBag.StatusMessage</p>

<p>You're logged in as <strong>@User.Identity.Name</strong>.</p>

@Html.Partial("_ChangePasswordPartial",Model.Hasło)

<h2>Twoje role</h2>
@foreach (var rola in Model.Role)
{
    @rola
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
